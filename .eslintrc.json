{
	"extends": "airbnb",
	"rules": {
		"indent": ["error", "tab"],
		"no-var": "error",
		"one-var": 0,
		"no-else-return": 0,
		"prefer-rest-params": 0,
		"new-cap": ["error", {"capIsNewExceptions": ["Class"]}],
		"comma-dangle": 0,
		"spaced-comment": 0,
		"no-param-reassign": 0,
		"no-cond-assign": 0
	},
	"env": {
		"browser": true,
		"jasmine": true

	},
	"globals": {
		"nofn": true
	}

}
